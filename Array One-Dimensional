package exemploarray;
public class ExemploArray {
    public static void main(String[] args) {
        String name[] = new String[3];
        
        //manually each position of the array from 0 of 2 ( three positions)
        
        name[0] = "Karoline";
        
        name [1] = "Lucas";
        
        name [2] = "Jessica";
        
        //and to acess each position we can use a tie from 0 to 2 (0.1.2-three positions)
        
        for(int i = 0; i <= 2; i++){
            System.out.print(" In position " +i" of array, we have: ");
            
            System.out.println(nomes[i]);
        }
    }
    
}
